{
  "Serilog": {
    "Properties": {
      "Application": "ArmoniK.PollingAgent.Worker"
    }
  },
  "ComputePlane": {
    "WorkerChannel": {
      "Address": "/cache/armonik_worker.sock",
      "SocketType": "unixdomainsocket"
    },
    "AgentChannel": {
      "Address": "/cache/armonik_agent.sock",
      "SocketType": "unixdomainsocket"
    }
  },
  "Authenticator": {
    "CNHeader": "X-Certificate-Client-CN",
    "FingerprintHeader": "X-Certificate-Client-Fingerprint",
    "ImpersonationUsernameHeader": "X-Impersonate-Username",
    "ImpersonationIdHeader": "X-Impersonate-Id",
    "RequireAuthentication": false,
    "RequireAuthorization": false
  }
}
